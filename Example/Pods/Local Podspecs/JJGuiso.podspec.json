{
  "name": "JJGuiso",
  "version": "1.5.8",
  "summary": "Easy way to load images and gif.",
  "description": "Guiso is image loading framework for IOS that wraps media decoding, memory and disk caching. By default Guiso uses URLSession.",
  "homepage": "https://github.com/Only-IceSoul/Guiso",
  "license": {
    "type": "Apache License 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "only-icesoul": "justinjlf21@gmail.com"
  },
  "source": {
    "git": "https://github.com/Only-IceSoul/Guiso.git",
    "tag": "1.5.8"
  },
  "requires_arc": true,
  "platforms": {
    "ios": "11.0"
  },
  "source_files": [
    "src/**/*.swift",
    "Frameworks/WebP.framework/Headers/*.{h}"
  ],
  "swift_versions": "5.0",
  "vendored_frameworks": "Frameworks/*.framework",
  "ios": {
    "preserve_paths": "Frameworks/*.framework"
  },
  "public_header_files": "Frameworks/WebP.framework/Headers/*.{h}",
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "swift_version": "5.0"
}
